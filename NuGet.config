<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <!-- COA Azure DevOps Feed -->
    <add key="COA" value="https://pkgs.dev.azure.com/COA/_packaging/COA/nuget/v3/index.json" />
    <!-- NuGet.org for other packages -->
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
  </packageSources>
  
  <packageSourceMapping>
    <!-- COA packages come from COA feed -->
    <packageSource key="COA">
      <package pattern="COA.*" />
    </packageSource>
    <!-- Everything else from NuGet.org -->
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
  </packageSourceMapping>
</configuration>